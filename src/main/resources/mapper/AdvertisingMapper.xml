<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.business.model.dao.AdvertisingMapper">

    <select id="pageList" resultType="com.business.controller.pc.advertising.vo.AdvertisingVO">
        SELECT * FROM advertising
        ORDER BY create_time DESC
    </select>

    <select id="getDownloadBeforeAdv" resultType="com.business.controller.mobile.advertising.vo.MobileAdvertisingVO">
        SELECT * FROM advertising ORDER BY RAND() LIMIT 1;
    </select>
</mapper>